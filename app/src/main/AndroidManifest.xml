<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="g.com.codinghousechallenges">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/codinghouseicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".CodinghouseActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".ChallengeActivities.ArgumentChallenge" />
        <activity android:name=".ChallengeActivities.CaseSwapChallenge" />
        <activity android:name=".ChallengeActivities.CounterFunctionChallenge" />
        <activity android:name=".ChallengeActivities.RevWordInPlaceChallenge"></activity>
    </application>

</manifest>